/// url = 'http://hl7.org/fhir/StructureMap/MetadataResource5to4'
/// name = 'MetadataResource5to4'
/// title = 'FML Conversion for MetadataResource: R5 to R4'

uses "http://hl7.org/fhir/StructureDefinition/MetadataResource" alias MetadataResourceR5 as source
uses "http://hl7.org/fhir/4.0/StructureDefinition/MetadataResource" alias MetadataResourceR4 as target

imports "http://hl7.org/fhir/StructureMap/*5to4"

group MetadataResource(source src : MetadataResourceR5, target tgt : MetadataResourceR4) extends DomainResource <<type+>> {
  src.approvalDate -> tgt.approvalDate;
  src.lastReviewDate -> tgt.lastReviewDate;
  src.effectivePeriod -> tgt.effectivePeriod;
  src.topic -> tgt.topic;
  src.author -> tgt.author;
  src.editor -> tgt.editor;
  src.reviewer -> tgt.reviewer;
  src.endorser -> tgt.endorser;
  src.relatedArtifact -> tgt.relatedArtifact;
}

