      <path value="Extension.url"/>
      <fixedUri value="http://hl7.org/fhir/StructureDefinition/organization-portal"/>
    </element>
    <element id="Extension.value[x]">
      <path value="Extension.value[x]"/>
      <max value="0"/>
    </element>
