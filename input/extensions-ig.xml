<?xml version="1.0" encoding="UTF-8"?>
<!-- Start by finding all references to "myig" and updating to appropriate text for your IG, including changing realm -->
<ImplementationGuide xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../input-cache/schemas/R4/fhir-single.xsd">
  <id value="hl7.fhir.extensions"/>
  <url value="http://hl7.org/fhir/extensions/ImplementationGuide/hl7.fhir.uv.extensions"/>
  <!-- This version will propagate to all artifacts unless the "propagate-version" extension is overridden -->
  <version value="0.1.0"/>
  <name value="FHIRGeneralExtensionsIG"/>
  <title value="FHIR General Extensions IG"/>
  <status value="active"/>
  <experimental value="false"/>
  <publisher value="HL7 International / FHIR Infrastructure"/>
  <contact>
    <telecom>
      <!-- Or whatever URL and/or email address(es) are appropriate -->
      <system value="url"/>
      <value value="http://hl7.org/Special/committees/fhir-i"/>
    </telecom>
  </contact>
  <description value="This IG defines the global extensions - the ones defined for everyone. These extensions are always in scope wherever FHIR is being used"/>
  <jurisdiction>
    <coding>
      <system value="http://unstats.un.org/unsd/methods/m49/m49.htm"/>
      <code value="001"/>
    </coding>
  </jurisdiction>
  <packageId value="hl7.fhir.uv.extensions"/>
  <license value="CC0-1.0"/>
  <fhirVersion value="5.0.0"/>
  <definition>
    <page>
      <sourceUrl value="index.html"/>
      <name value="index.html"/>
      <title value="Home Page"/>
      <generation value="html"/>
      <page>
        <sourceUrl value="other.html"/>
        <name value="other.html"/>
        <title value="Other Information"/>
        <generation value="markdown"/>
      </page>
    </page>
    <!-- copyright year is a mandatory parameter -->
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/tools/CodeSystem/ig-parameters"/>
        <code value="copyrightyear"/>
      </code>
      <value value="2022+"/>
    </parameter>
    <!-- releaselabel should be the ballot status for HL7-published IGs. -->
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/tools/CodeSystem/ig-parameters"/>
        <code value="releaselabel"/>
      </code>
      <value value="Working Copy"/>
    </parameter>
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/guide-parameter-code"/>
        <code value="path-resource"/>
      </code>
      <value value="input/definitions/*"/>
    </parameter>
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/tools/CodeSystem/ig-parameters"/>
        <code value="showsource"/>
      </code>
      <value value="true"/>
    </parameter>
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/tools/CodeSystem/ig-parameters"/>
        <code value="generate-version"/>
      </code>
      <value value="r2"/>
    </parameter>
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/tools/CodeSystem/ig-parameters"/>
        <code value="generate-version"/>
      </code>
      <value value="r3"/>
    </parameter>
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/tools/CodeSystem/ig-parameters"/>
        <code value="generate-version"/>
      </code>
      <value value="r4"/>
    </parameter>
    <parameter>
      <code>
        <system value="http://hl7.org/fhir/tools/CodeSystem/ig-parameters"/>
        <code value="generate-version"/>
      </code>
      <value value="r5"/>
    </parameter>    
  </definition>
</ImplementationGuide>
